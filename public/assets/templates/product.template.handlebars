<h5> <small class="row justify-content-center text-muted mt-2" id="products__title" >Lista de productos</h5>
<table class="table table-striped table-sm-bordered">
  <thead >
    <tr class="table-primary">
      <th scope="col">Titulo</th>
      <th scope="col">Precio</th>
      <th scope="col">Imagen</th>
    </tr>
  </thead>
  <tbody>
    {{#if products.length}}
        {{#each products}}
    <tr>
      <td>{{this.titulo}}</td>
      <td>{{this.precio}}</td>
      <td><img height="20px" width="20px" src={{this.imagen}} /></td>
    </tr>
     {{/each}}
    {{/if}}
 
  </tbody>
</table>


